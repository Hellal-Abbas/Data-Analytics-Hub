<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="65.0" prefWidth="571.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.DeletePostController">
   <children>
      <Label alignment="CENTER" layoutX="-79.0" layoutY="2.0" prefHeight="45.0" prefWidth="177.0" text="Delete Post">
         <font>
            <Font name="Calibri Bold" size="35.0" />
         </font>
      </Label>
      <Button layoutY="11.0" mnemonicParsing="false" onAction="#DeletePostHandler" prefHeight="35.0" prefWidth="105.0" style="-fx-background-color: white;" text="Delete" translateX="465.0">
         <effect>
            <DropShadow blurType="ONE_PASS_BOX" height="10.0" radius="4.5" width="10.0" />
         </effect>
         <font>
            <Font name="Calibri Bold" size="14.0" />
         </font>
      </Button>
      <Label alignment="CENTER_RIGHT" layoutX="126.0" layoutY="18.0" prefHeight="14.0" prefWidth="106.0" text="Enter Post ID:">
         <font>
            <Font name="Calibri" size="17.0" />
         </font>
      </Label>
      <TextField fx:id="postidField" layoutX="243.0" layoutY="16.0" prefHeight="25.0" prefWidth="200.0" promptText="Number etc. 1234">
         <font>
            <Font name="Calibri" size="12.0" />
         </font></TextField>
      <Label fx:id="deleteOutput" alignment="CENTER" layoutX="244.0" layoutY="43.0" prefHeight="17.0" prefWidth="200.0" textFill="#cc0000">
         <font>
            <Font name="Calibri" size="17.0" />
         </font></Label>
   </children>
</AnchorPane>
